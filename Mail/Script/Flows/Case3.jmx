		ctx.getThread().stop();
		}

}
else{	
		int inbox_activity_count=Integer.parseInt(vars.get(&quot;lt_activity_Id_matchNr&quot;).trim());
		//log.info(&quot;${lt_user_id}&quot;+&quot;:- Current activities in inbox : &quot;+inbox_activity_count +&quot; Current SelectedActivity: &quot;+&quot;${lt_act_id}&quot;);
		if(inbox_activity_count&lt;2 &amp;&amp; refreshcountint &lt;15 )
		{
			vars.put(&quot;clickrefreshflag&quot;,&quot;true&quot;);
			vars.put(&quot;QuickPullFlag&quot;,&quot;true&quot;);
