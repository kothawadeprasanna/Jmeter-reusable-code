              <stringProp name="BeanShellSampler.query">//This BS is for trimming the reply content
String temp=vars.get(&quot;lt_reply_Content_to_add&quot;);
String Original_reply= vars.get(&quot;lt_Agent_reply_msg_content&quot;);
//log.info(&quot;temp:- &quot;+temp);
//log.info(&quot;Original_reply:- &quot;+Original_reply+&quot; Length= &quot; + Original_reply.length());
String intialpointer= vars.get(&quot;lt_reply_pointer&quot;);
//log.info(&quot;intialpointer:- &quot;+intialpointer);
int i=Integer.parseInt(intialpointer);
//log.info(&quot;i:- &quot;+i);
int y=i+(Original_reply.length()/60);
//log.info(&quot;y:- &quot;+y);
//log.info(&quot;Original_reply: &quot;+Original_reply+&quot;\n Trimmed:&quot;+Original_reply.substring(0,10));

String trimmed_reply= temp.concat(Original_reply.substring(i, y));
vars.put(&quot;lt_reply_Content_to_add&quot;,trimmed_reply);
vars.put(&quot;lt_reply_pointer&quot;,String.valueOf(y));
//log.info(&quot;trimmed_reply1 &quot;+trimmed_reply);</stringProp>
